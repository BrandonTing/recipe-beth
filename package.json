{
  "name": "test",
  "module": "src/index.tsx",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"bun run liveReload\" \"bun run --hot src/index.tsx\"  ",
    "liveReload": "bunx beth-stack",
    "start": "bun run src/index.tsx",
    "tw": "bunx tailwindcss -i ./src/styles.css -o ./public/dist/styles.css",
    "tw:dev": "bunx tailwindcss -i ./src/styles.css -o ./public/dist/styles.css --watch",
    "typecheck": "bunx --bun tsc",
    "format:check": "prettier --check .",
    "format": "prettier --write . --list-different",
    "lint": "eslint . --report-unused-disable-directives",
    "lint:fix": "eslint . --report-unused-disable-directives --fix",
    "generate": "drizzle-kit generate:sqlite --out ./drizzle/migrations --breakpoints --schema=./src/db/schema.ts",
    "migrate": "bun drizzle/migrate.ts"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "latest",
    "@ianvs/prettier-plugin-sort-imports": "^4.1.0",
    "@kitajs/ts-html-plugin": "^1.0.1",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/eslint": "^8.44.2",
    "@typescript-eslint/parser": "^6.17.0",
    "bun-types": "latest",
    "concurrently": "^8.2.1",
    "drizzle-kit": "^0.19.13",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-turbo": "^1.10.14",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "pino": "^8.16.1",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@bogeychan/elysia-logger": "0.0.12",
    "@elysiajs/static": "^0.7.1",
    "@libsql/client": "^0.3.6",
    "@t3-oss/env-core": "^0.7.1",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "beth-stack": "0.0.33",
    "date-fns": "^2.30.0",
    "drizzle-orm": "^0.28.6",
    "elysia": "^0.7.17",
    "pino-pretty": "^10.2.0",
    "tailwind-merge": "^2.2.0",
    "tailwindcss": "^3.3.5",
    "zod": "^3.22.4"
  }
}